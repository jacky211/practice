		<script type="text/javascript">
		function sum()
		{  var result=0;
			var i=0;
			
			for(i=0; i<arguments.length; i++){
				
				
				result+=arguments[i];
			}
			
			
			alert(result);
			
		}
		sum(1,2,3)
		</script> 
